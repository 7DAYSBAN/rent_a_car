@model IEnumerable<Rent_A_Car.CarBrand>

@{
    ViewData["Title"] = "Brands";
}

<h1>View</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CarBrandName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
     @foreach (var item in Model) 
{
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CarBrandName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CarBrandId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CarBrandId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CarBrandId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
